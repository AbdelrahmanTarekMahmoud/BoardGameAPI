{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "MyBGList": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "MyBGList": "Information"
      }
    }
  },
  "JWT": {
    "Issuer": "MyBGList",
    "Audience": "MyBGList",
    "SigningKey": "SuperLongSecretKeyThatIsAtLeast32Chars"
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "*",
  "UseDeveloperExceptionPage": true, //change it to false
  "UseSwagger": false
}
